						SET Blocco2_Office_Wall_2_0_0_0_0_0_Win_Shading_Deploy_Status = Shade_Status_Off,
					ENDIF,
				ENDIF,
			ENDIF,
		ENDIF,
	ELSE,
		IF Surface_Outside_Face_Incident_Solar_Radiation_Rate_per_Area > 150,
